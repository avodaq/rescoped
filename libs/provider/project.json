{
  "name": "provider",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/provider",
  "prefix": "avo",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/provider/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/provider/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/provider/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/provider/**/*.{ts,tsx,js,jsx}", "libs/provider/package.json"]
      }
    }
  },
  "tags": []
}
