{
  "name": "rescoped",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps --base=remotes/origin/main",
    "affected:build": "nx affected:build --base=remotes/origin/main",
    "affected:dep-graph": "nx affected:dep-graph --base=remotes/origin/main",
    "affected:e2e": "nx affected:e2e --base=remotes/origin/main",
    "affected:libs": "nx affected:libs --base=remotes/origin/main",
    "affected:lint": "nx affected:lint --parallel --base=remotes/origin/main",
    "affected:lint:watch": "nx affected:lint --watch --parallel --base=remotes/origin/main",
    "affected:test": "nx affected:test --parallel --base=remotes/origin/main",
    "affected:test:headless": "nx affected:test --watch=false --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:parallel": "nx affected:test --watch=false --parallel --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:parallel:watch": "nx affected:test --watch --parallel --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:watch": "nx affected:test --watch --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:watch": "nx affected:test --watch --parallel --base=remotes/origin/main",
    "build": "ng build",
    "build:all": "rm -rf dist && npx nx run-many -t build --all --skip-nx-cache && cp -R libs/eslint-config dist/libs/eslint-config",
    "build:lib:components": "nx build components",
    "build:prod": "ng build $npm_config_project --configuration=production",
    "build:prod:serve": "set NODE_OPTIONS=--max_old_space_size=8192 & nx serve $npm_config_project --configuration=production",
    "build:staging": "NB_BUILD_MANGLE=false ng build $npm_config_project --configuration=staging",
    "build:staging:serve": "set NODE_OPTIONS=--max_old_space_size=8192 & NB_BUILD_MANGLE=false nx serve $npm_config_project --configuration=staging",
    "dep-graph": "nx dep-graph",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "husky install",
    "lint": "nx workspace-lint && ng lint",
    "lint-staged": "lint-staged",
    "lint:all": "nx run-many -t lint --all",
    "lint:all:parallel": "nx run-many -t lint --all --parallel",
    "ncu": "ncu -i",
    "ng": "nx",
    "nx": "nx",
    "nx:generate:app": "nx generate app",
    "nx:generate:lib": "nx generate lib",
    "nx:move": "nx generate move --projectName",
    "nx:remove": "nx generate remove --forceRemove --projectName",
    "skygrid:build:production": "nx run skygrid:build:production",
    "skygrid:dev:serve": "nx serve skygrid",
    "skygrid:e2e": "nx run skygrid-e2e:e2e --browser=chrome --watch",
    "skygrid:e2e:headless": "nx run skygrid-e2e:e2e --browser=chrome",
    "skygrid:prod:serve": "nx serve skygrid --configuration=production",
    "skygrid:staging:serve": "nx serve skygrid --configuration=staging",
    "sort": "sort-package-json",
    "start": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve",
    "test": "ng test",
    "test:all": "nx run-many -t test --all --watch=false",
    "test:all:headless": "nx run-many -t test --all --browsers=ChromeHeadlessCI --watch=false",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator"
  },
  "lint-staged": {
    "*.{ts,html,scss,json}": [
      "nx affected:lint --parallel --uncommitted",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "~18.0.7",
    "@angular/cdk": "~18.0.6",
    "@angular/common": "~18.0.7",
    "@angular/compiler": "~18.0.7",
    "@angular/core": "~18.0.7",
    "@angular/forms": "~18.0.7",
    "@angular/material": "~18.0.6",
    "@angular/material-moment-adapter": "~18.0.6",
    "@angular/platform-browser": "~18.0.7",
    "@angular/platform-browser-dynamic": "~18.0.7",
    "@angular/router": "~18.0.7",
    "@material/typography": "~15.0.0-canary.7971d6ad5.0",
    "@ngrx/component-store": "^18.0.1",
    "@ngrx/entity": "^18.0.1",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/store": "^18.0.1",
    "@ngx-pwa/local-storage": "~18.0.0",
    "@nx/angular": "~19.4.2",
    "@nx/devkit": "~19.4.2",
    "@rescoped/components": "file:libs/components",
    "@rescoped/eslint-config": "file:../rescoped-builds/eslint-config",
    "@rescoped/fonts": "file:libs/fonts",
    "@rescoped/styles": "file:libs/styles",
    "@rescoped/theme-style": "file:libs/styles/theming",
    "deepmerge": "^4.3.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "ng-table-virtual-scroll": "^1.6.1",
    "ngx2-pipes": "^13.0.0",
    "rxjs": "~7.8.1",
    "tslib": "~2.5.3",
    "zone.js": "~0.14.7"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.1800.7",
    "@angular-devkit/build-angular": "~18.0.7",
    "@angular-devkit/core": "~18.0.7",
    "@angular-devkit/schematics": "~18.0.7",
    "@angular-eslint/eslint-plugin": "~18.1.0",
    "@angular-eslint/eslint-plugin-template": "~18.1.0",
    "@angular-eslint/template-parser": "~18.1.0",
    "@angular/cli": "~18.0.0",
    "@angular/compiler-cli": "~18.0.7",
    "@angular/language-service": "~18.0.7",
    "@mswjs/data": "~0.16.1",
    "@nx/cypress": "~19.4.2",
    "@nx/eslint": "~19.4.2",
    "@nx/eslint-plugin": "~19.4.2",
    "@nx/jest": "~19.4.2",
    "@nx/js": "~19.4.2",
    "@nx/node": "~19.4.2",
    "@nx/plugin": "~19.4.2",
    "@nx/workspace": "~19.4.2",
    "@schematics/angular": "~18.0.7",
    "@swc-node/register": "1.9.2",
    "@swc/cli": "0.3.14",
    "@swc/core": "1.5.7",
    "@swc/helpers": "0.5.11",
    "@testing-library/angular": "^17.0.0",
    "@types/jasmine": "~4.3.4",
    "@types/jest": "^29.5.11",
    "@types/karma": "^6.3.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "~18.16.9",
    "@types/prompts": "~2.4.9",
    "@types/webpack": "~5.28.5",
    "@typescript-eslint/eslint-plugin": "7.9.0",
    "@typescript-eslint/parser": "7.9.0",
    "@typescript-eslint/utils": "^8.0.0-alpha.28",
    "autoprefixer": "^10.4.19",
    "codelyzer": "6.0.2",
    "colors": "1.4.0",
    "commitizen": "4.3.0",
    "css-loader": "6.9.0",
    "cypress": "13.13.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.15.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-rxjs": "5.0.3",
    "eslint-plugin-rxjs-angular": "2.0.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "git-cz": "4.9.0",
