{
  "name": "avo",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps --base=remotes/origin/main",
    "affected:build": "nx affected:build --base=remotes/origin/main",
    "affected:dep-graph": "nx affected:dep-graph --base=remotes/origin/main",
    "affected:e2e": "nx affected:e2e --base=remotes/origin/main",
    "affected:libs": "nx affected:libs --base=remotes/origin/main",
    "affected:lint": "nx affected:lint --parallel --base=remotes/origin/main",
    "affected:lint:watch": "nx affected:lint --watch --parallel --base=remotes/origin/main",
    "affected:test": "nx affected:test --parallel --base=remotes/origin/main",
    "affected:test:headless": "nx affected:test --watch=false --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:parallel": "nx affected:test --watch=false --parallel --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:watch": "nx affected:test --watch --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:headless:parallel:watch": "nx affected:test --watch --parallel --browsers=ChromeHeadlessCI --base=remotes/origin/main",
    "affected:test:watch": "nx affected:test --watch --parallel --base=remotes/origin/main",
    "build": "ng build",
    "build:prod": "ng build $npm_config_project --configuration=production",
    "build:prod:serve": "set NODE_OPTIONS=--max_old_space_size=8192 & nx serve $npm_config_project --configuration=production",
    "build:staging": "NB_BUILD_MANGLE=false ng build $npm_config_project --configuration=staging",
    "build:staging:serve": "set NODE_OPTIONS=--max_old_space_size=8192 & NB_BUILD_MANGLE=false nx serve $npm_config_project --configuration=staging",
    "dep-graph": "nx dep-graph",
    "skygrid:e2e": "nx run skygrid-e2e:e2e --browser=chrome --watch",
    "skygrid:e2e:headless": "nx run skygrid-e2e:e2e --browser=chrome",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "help": "nx help",
    "postinstall": "ngcc --properties es2020 browser module main && husky install",
    "lint": "nx workspace-lint && ng lint",
    "ncu": "ncu -i",
    "ng": "nx",
    "nx": "nx",
    "nx:generate:app": "nx generate app",
    "nx:generate:lib": "nx generate lib",
    "nx:move": "nx generate move --projectName",
    "nx:remove": "nx generate remove --forceRemove --projectName",
    "sort": "sort-package-json",
    "start": "set NODE_OPTIONS=--max_old_space_size=8192 & ng serve",
    "test": "ng test",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "setup:virtualrepo": "nx workspace-generator virtualrepo",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,html}": [
      "nx affected:lint --parallel --uncommitted",
      "prettier --write"
    ],
    "*.{json,md,js,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "15.1.1",
