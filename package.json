    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "karma-junit-reporter": "2.0.1",
    "lint-staged": "^13.2.2",
    "mochawesome": "^7.1.3",
    "ng-mocks": "^14.13.0",
    "ng-packagr": "~18.0.0",
    "npm-check-updates": "^16.14.20",
    "nx": "~19.4.2",
    "postcss": "^8.4.39",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^9.6.0",
    "postcss-url": "^10.1.3",
    "prettier": "^3.3.2",
    "prompts": "2.4.2",
    "sort-package-json": "^2.10.0",
    "stylelint": "^16.6.1",
    "stylelint-scss": "^6.4.1",
    "sw2dts": "2.6.3",
    "tailwindcss": "^3.4.4",
    "ts-expect": "~1.3.0",
    "ts-jest": "^29.1.1",
    "ts-node": "~10.9.2",
    "tsconfig-paths-webpack-plugin": "~4.1.0",
